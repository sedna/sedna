	$(mkinstalldirs) $(DESTDIR)$(pkgdatadir)/doc
	-for file in $(DOCFILES); do cp $(srcdir)/$$file $(DESTDIR)$(pkgdatadir)/doc; done
	$(mkinstalldirs) $(DESTDIR)$(pkglibdir)

