chicken-config: chicken-config.in
	sed_quote_subst='s/\([\\`\\"$\\\\]\)/\\\1/g' ; \
	quoted_cflags=`echo X$(CFLAGS) $(BASE_CPPFLAGS) | sed -e s/^X// -e "$$sed_quote_subst"` ; \
	quoted_cppflags=`echo X$(CPPFLAGS) $(BASE_CPPFLAGS) | sed -e s/^X// -e "$$sed_quote_subst"` ; \
	sed -e "s,%cflags%,$$quoted_cflags," \
	    -e "s,%cc%,$(CC)," \
	    -e "s,%cxx%,$(CXX)," \
	    -e "s,%includedir%,$(includedir)," \
	    -e "s!%morelibs%!$(MORE_LIBS) $(EXTRA_LINK_FLAGS)!" \
	    -e "s,%libdir%,$(libdir),g" \
	    -e "s,%morestaticlibs%,$(MORE_STATIC_LIBS)," \
	    -e "s,%CHICKEN_HOME%,$(pkgdatadir)," \
	    -e "s,%CHICKEN_LIB_HOME%,$(pkglibdir)," \
	    -e "s,%version%,$(VERSION)," \
	    -e "s,%prefix%,$(prefix)," \
	  <$(srcdir)/chicken-config.in >chicken-config
	chmod a+x chicken-config

csc.scm: csc.scm.in
	sed_quote_subst='s/\([\\`\\"$\\\\]\)/\\\1/g' ; \
	quoted_cflags=`echo X$(CFLAGS) $(BASE_CPPFLAGS) | sed -e s/^X// -e "$$sed_quote_subst"` ; \
	quoted_cppflags=`echo X$(CPPFLAGS) $(BASE_CPPFLAGS) | sed -e s/^X// -e "$$sed_quote_subst"` ; \
	sed -e "s,%bindir%,$(DESTDIR)$(bindir)," \
	    -e "s,%cc%,$(CC)," \
	    -e "s,%cxx%,$(CXX)," \
	    -e "s,%cxxlinker%,$(CXX)," \
	    -e "s,%cflags%,$$quoted_cflags," \
	    -e "s,%morelibs%,$(MORE_LIBS)," \
	    -e "s,%morestaticlibs%,$(MORE_STATIC_LIBS)," \
	    -e "s,%CHICKEN_HOME%,$(pkgdatadir)," \
	    -e "s,%includedir%,$(includedir)," \
	    -e "s,%libdir%,$(libdir),g" \
	    -e "s,%optlibs%,#f," \
	  <$(srcdir)/csc.scm.in >csc.scm

