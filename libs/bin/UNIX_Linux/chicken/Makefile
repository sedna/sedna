c_defaults.h: nsample
	@if [ -n "${NURSERY}" ]; then \
	  nursery=${NURSERY}; \
	  echo "setting nursery size to $${nursery}"; \
	else \
	  echo "checking for best nursery size ..."; \
	  bestn=0; \
	  for n in 8 10 12 16 24 32 48 64 128 256 512 1024; do \
	    tm1=`$(NSAMPLE) -:s$${n}k`; \
	    tm2=`$(NSAMPLE) -:s$${n}k`; \
	    tm3=`$(NSAMPLE) -:s$${n}k`; \
	    tm=`expr \( $$tm1 + $$tm2 + $$tm3 \) / 3`; \
	    echo " $${n}K -> $${tm} ms"; \
	    if [ $$bestn -eq 0 ]; then \
              best=$$tm; \
	      bestn=$$n; \
            elif [ $$tm -le $$best ]; then \
	      best=$$tm; \
	      bestn=$$n; \
	    fi; \
	  done; \
	  if [ $$bestn -eq 0 ]; then \
            nursery="256*1024"; \
            echo "hm, no decent nursery value here ... using 256K"; \
	  else \
	    echo the optimal size of the nursery seems to be $${bestn}K; \
	    nursery="$${bestn}*1024"; \
          fi; \
	fi; \
	echo the CHICKEN_HOME install directory is ${pkgdatadir}; \
	echo creating $@; \
	echo "/*** Program generated file: any edits may be lost ***/" >c_defaults.h; \
	echo "#define C_INSTALL_HOME \"${pkgdatadir}\"" >>c_defaults.h; \
	echo "#ifndef C_INSTALL_LIB_HOME" >> c_defaults.h; \
	echo "# define C_INSTALL_LIB_HOME \"${pkglibdir}\"" >>c_defaults.h; \
	echo "#endif" >> c_defaults.h; \
	echo "#define C_BIN_DIR \"$(DESTDIR)$(bindir)\"" >>c_defaults.h; \
	echo "#ifndef C_DEFAULT_TARGET_STACK_SIZE" >> c_defaults.h; \
	echo "# define C_DEFAULT_TARGET_STACK_SIZE ($${nursery})" >>c_defaults.h; \
	echo "#endif" >> c_defaults.h; \
	echo "#define C_CC \"$(CC)\"" >> c_defaults.h; \
	echo "#ifndef C_STACK_GROWS_DOWNWARD" >> c_defaults.h; \
	echo "# define C_STACK_GROWS_DOWNWARD (${STACK_GROWS_DOWNWARD})" >> c_defaults.h; \
	echo "#endif" >> c_defaults.h; \
	echo "#define C_INSTALL_BIN_HOME \"$(DESTDIR)${bindir}\"" >>c_defaults.h

nsample.c: nsample.scm
