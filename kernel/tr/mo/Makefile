#
# Makefile for mo
#

PP = ../../..

include $(PP)/Makefile.include

all: mo$(LIB_EXT)
	@echo ===================================================================
	@echo mo DONE
	@echo ===================================================================

OBJS = micro$(OBJ_EXT) mo_write_utils$(OBJ_EXT)

ifneq ($(findstring clean, $(MAKECMDGOALS)), clean)
include $(OBJS:$(OBJ_EXT)=.d)
endif

################################################################################
# mo.lib                                                                       #
################################################################################
mo$(LIB_EXT): $(OBJS)
	$(LB) $(LIBFLAGS) $(LIBOUT)$@ $^

################################################################################
# Clean                                                                        #
################################################################################
.PHONY: clean

clean: generic_clean
