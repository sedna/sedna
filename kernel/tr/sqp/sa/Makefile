#
# Makefile for sa
#

PP = ../../../..

include $(PP)/Makefile.include

all: sa$(LIB_EXT)
	@echo ===================================================================
	@echo sa DONE
	@echo ===================================================================

OBJS = sa$(OBJ_EXT)

ifneq ($(findstring clean, $(MAKECMDGOALS)), clean)
include $(OBJS:$(OBJ_EXT)=.d)
endif

################################################################################
# sa.lib                                                                       #
################################################################################
sa$(LIB_EXT): $(OBJS)
	$(PLD) $(PLDOUT)$@ $^

################################################################################
# Clean                                                                        #
################################################################################
.PHONY: clean

clean: generic_clean
	-$(REMOVE) sa.c
