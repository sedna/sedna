#
# Makefile for vmm
#

PP = ../../..

include $(PP)/Makefile.include

all: vmm$(LIB_EXT)
	@echo ===================================================================
	@echo vmm DONE
	@echo ===================================================================

OBJS = os_exceptions$(OBJ_EXT) vmm$(OBJ_EXT)

ifneq ($(findstring clean, $(MAKECMDGOALS)), clean)
include $(OBJS:$(OBJ_EXT)=.d)
endif

################################################################################
# vmm.lib                                                                      #
################################################################################
vmm$(LIB_EXT): $(OBJS)
	$(LB) $(LIBFLAGS) $(LIBOUT)$@ $^

################################################################################
# Clean                                                                        #
################################################################################
.PHONY: clean

clean: generic_clean
