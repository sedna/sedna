#
# Makefile for lip (GNU make)
#

PP = ../../../../..

include $(PP)/Makefile.include

all: lip$(LIB_EXT)
	@echo ===================================================================
	@echo lip DONE
	@echo ===================================================================

OBJS = lip$(OBJ_EXT)

ifneq ($(findstring clean, $(MAKECMDGOALS)), clean)
include $(OBJS:$(OBJ_EXT)=.d)
endif

################################################################################
# CRMUTILS                                                                          #
################################################################################
lip$(LIB_EXT): $(OBJS)
	$(PLD) $(PLDOUT)$@ $^


################################################################################
# Clean                                                                        #
################################################################################
.PHONY: clean

clean: generic_clean
