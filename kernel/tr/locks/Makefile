#
# Makefile for locks
#

PP = ../../..

include $(PP)/Makefile.include

all: locks$(LIB_EXT)
	@echo ===================================================================
	@echo locks DONE
	@echo ===================================================================

OBJS = locks$(OBJ_EXT)

ifneq ($(findstring clean, $(MAKECMDGOALS)), clean)
include $(OBJS:$(OBJ_EXT)=.d)
endif

################################################################################
# locks.lib                                                                    #
################################################################################
locks$(LIB_EXT): $(OBJS)
	$(LB) $(LIBFLAGS) $(LIBOUT)$@ $^


################################################################################
# Clean                                                                        #
################################################################################
.PHONY: clean

clean: generic_clean
