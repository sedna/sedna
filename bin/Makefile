#
# Makefile for bin (GNU make)
#

PP = ..


include $(PP)/Makefile.include


################################################################################
# Clean                                                                        #
################################################################################
.PHONY: clean

clean: generic_clean
	-$(REMOVE) se_cdb$(EXE_EXT)
	-$(REMOVE) se_ddb$(EXE_EXT)
	-$(REMOVE) se_gov$(EXE_EXT)
	-$(REMOVE) se_rc$(EXE_EXT)
	-$(REMOVE) se_sm$(EXE_EXT)
	-$(REMOVE) se_smsd$(EXE_EXT)
	-$(REMOVE) se_stop$(EXE_EXT)
	-$(REMOVE) se_trn$(EXE_EXT)
	-$(REMOVE) se_term$(EXE_EXT)
	-$(REMOVE) se_rcv$(EXE_EXT)
	-$(REMOVE) se_exp$(EXE_EXT)
